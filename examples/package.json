{
  "name": "everscale-graphql-examples",
  "version": "0.1.0",
  "description": "Everscale GraphQL code generation guide",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "yarn generate --watch \"src/**/*.ts\"",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "generate": "graphql-codegen --config codegen.ts"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.3.1",
    "typescript": "^5.0.4",
    "@graphql-codegen/typescript": "3.0.4"
  },
  "dependencies": {
    "@eversdk/core": "^1.42.1",
    "@eversdk/lib-node": "^1.42.1",
    "axios": "^1.4.0",
    "graphql": "^16.6.0"
  }
}
